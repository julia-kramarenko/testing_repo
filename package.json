{
  "name": "test-api",
  "version": "0.0.1",
  "description": "API for restfull test",
  "main": "dist",
  "scripts": {
    "dev": "SET NODE_ENV=development & nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "babel-version": "babel --version",
    "start": "SET NODE_ENV=producation pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "tsc \"api_tests\"specs & ./node_modules/mocha/bin/mocha api_tests/specs/crud_user.spec.js"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "type": "module",
  "author": "Yuliia Kramarenko",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^7.0.2",
    "@types/moment": "^2.13.0",
    "@types/superagent-proxy": "^2.0.0",
    "@types/yargs": "^15.0.5",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chance": "^1.1.4",
    "express": "^4.17.1",
    "mocha": "^7.1.2",
    "mongoose": "^5.9.11",
    "supertest": "^4.0.2",
    "webdriverio": "^6.1.12"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.9",
    "@wdio/cli": "^6.1.12",
    "@wdio/local-runner": "^6.1.12",
    "@wdio/mocha-framework": "^6.1.8",
    "@wdio/spec-reporter": "^6.1.12",
    "@wdio/sync": "^6.1.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chromedriver": "^81.0.0",
    "eslint": "^6.8.0",
    "gulp": "^4.0.2",
    "http": "0.0.1-security",
    "mocha-junit-reporter": "^1.23.3",
    "nodemon": "^2.0.3",
    "typings": "^2.1.1",
    "wdio-chromedriver-service": "^6.0.3"
  }
}
